float4	  SkyColor					 : register(c0);
sampler   SkyTexture                 : register(s0);

struct PS_INPUT
{
	float3 CubeTexcoord       : TEXCOORD0;
};

half4 ps_main(PS_INPUT Input) : COLOR0
{
	return SkyColor * texCUBE(SkyTexture, normalize(Input.CubeTexcoord));
}